{
  "name": "mercury",
  "version": "0.0.0-RELEASEVERSION",
  "private": true,
  "scripts": {
    "dev": "yarn build && concurrently \"yarn keycloak\" \"yarn server\" \"yarn saturn\" \"yarn elasticsearch\"",
    "dev-no-server": "yarn build && concurrently \"yarn keycloak\" \"yarn saturn\" \"yarn elasticsearch\"",
    "dev-no-saturn": "yarn build && concurrently \"yarn keycloak\" \"yarn server\" \"yarn elasticsearch\"",
    "keycloak": "cd ../../local-development && docker-compose up",
    "server": "wait-on http://localhost:5100 && sleep 3 && wait-on http://localhost:8080/api/v1/health/ && cd server/ && KEYCLOAK_CLIENT_SECRET=********** nodemon server.js",
    "saturn": "wait-on http://localhost:9200/ && cd ../saturn/ && ./gradlew run",
    "elasticsearch": "./start-elasticsearch",
    "start": "react-scripts start",
    "build": "react-scripts build && cd server && npm install",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --env=jsdom --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint ./src",
    "flow": "flow"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.11",
    "@material-ui/core": "^4.7.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "4.0.0-alpha.33",
    "@material-ui/pickers": "^3.2.8",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "date-fns": "2.1.0",
    "deepmerge": "^4.2.2",
    "elasticsearch": "^16.5.0",
    "filesize": "^6.0.1",
    "http-proxy-middleware": "^0.20.0",
    "jsonld": "^1.8.1",
    "lodash": "^4.17.15",
    "mdi-material-ui": "latest",
    "prop-types": "^15.7.2",
    "query-string": "^6.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-dropzone": "^10.2.0",
    "react-is-mounted-hook": "^1.0.2",
    "react-markdown": "^4.2.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-select": "^3.0.8",
    "typeface-roboto": "^0.0.75",
    "use-deep-compare-effect": "^1.3.0",
    "vis-network": "^6.4.4",
